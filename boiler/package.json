{
  "name": "electron-boiler",
  "version": "0.0.0",
  "description": "Electron bootstrap and jquery boilerplate",
  "author": "PatEvs",
  "license": "UNLICENSED",
  "main": "main.js",
  "scripts": {
    "clean": "npx trash-cli node_modules lib",
    "copy-libs": "mkdir lib && ncp node_modules/photonkit/ lib/photonkit/",
    "preinstall": "npm run clean",
    "postinstall": "npm run copy-libs",
    "prestart": "npm install",
    "start": "electron .",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "photonkit": "^0.1.2"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-reloader": "^1.0.1",
    "eslint": "^7.4.0",
    "ncp": "^2.0.0"
  }
}
